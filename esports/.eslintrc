{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-new": 0,
    "arrow-body-style": 0,
    "comma-dangle": [0],
    "import/no-unresolved": 0,
    "func-names": 0,
    "indent": [2, 2],
    "no-tabs": 0,
    "space-before-function-paren": 0,
    "class-methods-use-this": 0,
    "react/require-extension": "off",
    "prefer-destructuring": 0
  }
}
